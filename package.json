{
  "name": "socialverse-web",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "SocialVerse - A modern social networking platform",
  "authorSocialVerse Team",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "npm run build:safe",
    "build:safe": "npm run clean && npm run build:production",
    "build:production": "NODE_OPTIONS='--max-old-space-size=4096' nuxt build",
    "build:debug": "NODE_OPTIONS='--max-old-space-size=4096' DEBUG=nuxt:* nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "start": "node .output/server/index.mjs",
    "clean": "rm -rf .nuxt .output .nitro node_modules/.cache dist",
    "clean:full": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/tailwindcss": "^6..0",
    "@pinia/nuxt": "^0.11.2",
    "@supabase/supabase-js": "^.0",
    "bcrypt": "^5.1.",
    "chart.js": "^4.5.1",
    "compression": "^1.7.4",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "emoji-js": "^3.9.0",
    "ethers": "^6.15.0",
    "gun": "^0.2020.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "markdown-it": "^14.1.0",
    "multer": "^2.0.2",
    "nuxt": "^4.1.3",
    "pinia": "^3.0.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.22",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.6.3",
    "unhead": "^2.0.19",
    "web": "^4.11.0",
    "aws-sdk": "^2.1.0",
    "sharp": "^0.33.0",
    "@vitalets/google-translate-api": "^9.2.0"
  },
  "optionalDependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "@tensorflow/tfjs-layers": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.7.0",
    "@types/bcrypt": "^5.0.",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^.0",
    "prettier": "^3.2.5",
    "typescript": "^5.5.4",
    "vitest": "^1.0.0"
  }
}


