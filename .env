PREMIUM_PRICE=999
VIP_PRICE=1999

# ============================================================================
# PAYMENT PROCESSING
# ============================================================================
PAYMENT_PROVIDER=stripe
PAYMENT_CURRENCY=USD
PAYMENT_WEBHOOK_URL=https://your-domain.com/api/webhooks/payment

# ============================================================================
# APPLICATION CONFIGURATION
# ============================================================================
NODE_ENV=production
PORT=3000
HOST=0.0.0.0
NUXT_PUBLIC_API_BASE=https://api.socialverse.com
NUXT_PUBLIC_APP_URL=https://socialverse.com

# ============================================================================
# AUTHENTICATION
# ============================================================================
JWT_SECRET=your-jwt-secret-key-min-32-characters-long
SESSION_SECRET=your-session-secret-key-min-32-characters
AUTH_COOKIE_NAME=socialverse_auth
AUTH_COOKIE_SECURE=true
AUTH_COOKIE_HTTP_ONLY=true
AUTH_COOKIE_SAME_SITE=strict

# ============================================================================
# EMAIL CONFIGURATION (Brevo/SendinBlue)
# ============================================================================
BREVO_API_KEY=your-brevo-api-key
BREVO_SENDER_EMAIL=noreply@socialverse.com
BREVO_SENDER_NAME=SocialVerse

# ============================================================================
# LOGGING & MONITORING
# ============================================================================
LOG_LEVEL=info
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
ENABLE_ANALYTICS=true

# ============================================================================
# FEATURE FLAGS
# ============================================================================
ENABLE_PREMIUM_FEATURES=true
ENABLE_ML_RECOMMENDATIONS=true
ENABLE_CDN_UPLOADS=true
ENABLE_LOAD_BALANCING=true
ENABLE_AD_SERVING=true

# ============================================================================
# RATE LIMITING
# ============================================================================
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_STREAM_WINDOW=60000
RATE_LIMIT_STREAM_MAX=5

# ============================================================================
# CORS & SECURITY
# ============================================================================
CORS_ORIGIN=https://socialverse.com,https://www.socialverse.com
CORS_CREDENTIALS=true
CORS_METHODS=GET,POST,PUT,DELETE,PATCH,OPTIONS
CORS_HEADERS=Content-Type,Authorization

# ============================================================================
# FILE UPLOAD LIMITS
# ============================================================================
MAX_UPLOAD_SIZE=524288000
MAX_VIDEO_SIZE=5368709120
MAX_IMAGE_SIZE=52428800
ALLOWED_VIDEO_TYPES=video/mp4,video/webm,video/ogg
ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp,image/gif

# ============================================================================
# STREAM CONFIGURATION
# ============================================================================
STREAM_SEGMENT_DURATION=10
STREAM_PLAYLIST_SIZE=5
STREAM_BUFFER_SIZE=3
STREAM_TIMEOUT=30000
STREAM_QUALITY_LEVELS=360p,480p,720p,1080p

# ============================================================================
# DATABASE CONFIGURATION
# ============================================================================
DATABASE_URL=postgresql://user:password@localhost:5432/socialverse
DATABASE_POOL_SIZE=20
DATABASE_IDLE_TIMEOUT=30000

# ============================================================================
# CACHE CONFIGURATION
# ============================================================================
CACHE_TTL_SHORT=300
CACHE_TTL_MEDIUM=3600
CACHE_TTL_LONG=86400
CACHE_STRATEGY=redis

# ============================================================================
# EXTERNAL SERVICES
# ============================================================================
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
FACEBOOK_PIXEL_ID=123456789
TWITTER_API_KEY=your-twitter-api-key
TWITTER_API_SECRET=your-twitter-api-secret

# ============================================================================
# DEVELOPMENT ONLY
# ============================================================================
DEBUG=false
MOCK_SERVICES=false
SKIP_ENV_VALIDATION=false

